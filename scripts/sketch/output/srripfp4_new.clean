// Promotion
int[4] hitState (int[4] state, int pos) {
  int[4] final = state;
  if(state[pos] != 3)/*extra.sk:53*/
  {
    final[pos] = final[pos] + 1;
  }
  return final;
}

// Replacement: select block with smaller age
int missIdx (int[4] state) {
  int idx = 0;
  for(int i = 0; i < 4; i = i + 1) {
    if(id != i && (state[idx] > state[i])) {
      id = i;
    }
  }
  return id;
}

int[4] missState (int[4] state) {
  int replace = missIdx(state);
  int[4] final = state;
  bit found = 0;
  // Check if htere is an element to replace
  for(int j = 0; j < 4; j = j + 1) {
    if(found == 0) {
      for(int i = 0; i < 4; i = i + 1) {
        if(found == 0) {
          if(final[i] == 0) {
            found = 1;
          }
        }
      }
    }
    // If not, decrease all ages
    if(found == 0) {
      for(int i = 0; i < 4; i = i + 1) {
        final[i] = final[i] - 1;
      }
    }
  }
  // Insertion
  final[replace] = 1;
  return final;
}
